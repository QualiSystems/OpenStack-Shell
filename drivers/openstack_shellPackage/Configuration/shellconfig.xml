<?xml version="1.0" encoding="utf-8"?>
<ShellsConfiguration
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://schemas.qualisystems.com/ResourceManagement/ShellsConfigurationSchema.xsd">
    <ResourceTemplates>
        <ResourceTemplate Name="OpenStack" Model="OpenStack" Driver="OpenStack Shell Driver" AddressRequired="false">
            <Description>Template for creating OpenStack cloud Provider Resource</Description>
            <AutoLoad Enable="true">
                <Description>Checks that the inputs are valid for the target OpenStack</Description>
             </AutoLoad>
            <Attributes>
              <Attribute Name="Controller URL" Value="http://controller:5000/v3"/>
              <Attribute Name="OpenStack Domain Name" Value="default"/>
              <Attribute Name="OpenStack Project Name" Value=""/>
              <Attribute Name="User Name" Value=""/>
              <Attribute Name="Password" Value=""/>
              <Attribute Name="OpenStack Reserved Networks" Value="" />
              <Attribute Name="OpenStack Management Network ID" Value=""/>
              <Attribute Name="Vlan Type" Value=""/>
              <Attribute Name="OpenStack Physical Interface Name" Value=""/>
              <Attribute Name="Floating IP Subnet ID" Value=""/>
              <Attribute Value="" Name="Execution Server Selector"/>
            </Attributes>
        </ResourceTemplate>
    </ResourceTemplates>
</ShellsConfiguration>